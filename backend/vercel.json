{
  "version": 2,
  "functions": {
    "server.js": {
      "includeFiles": "**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "deploymentProtection": {
    "enabled": false
  }
}